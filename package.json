{
    "name": "@kant2002-diia-inhouse/analytics",
    "version": "1.54.1",
    "description": "Analytics utils",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "repository": "https://github.com/diia-open-source/be-pkg-analytics.git",
    "author": "Diia",
    "licence": "SEE LICENCE IN LICENCE.md",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc",
        "semantic-release": "semantic-release",
        "start": "npm run build && node dist/index.js",
        "lint": "eslint --ext .ts . && prettier --check .",
        "lint-fix": "eslint '*/**/*.{js,ts}' --fix && prettier --write .",
        "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts registry.npmjs.org --validate-https",
        "prepare": "npm run build",
        "test": "jest",
        "test:unit": "npm run test --selectProjects unit --",
        "test:coverage": "npm run test --coverage",
        "find-circulars": "madge --circular --extensions ts ./"
    },
    "dependencies": {
        "bson": "6.2.0"
    },
    "peerDependencies": {
        "@kant2002-diia-inhouse/types": ">=5.0.0",
        "@kant2002-diia-inhouse/utils": ">=2.2.0"
    },
    "devDependencies": {
        "@kant2002-diia-inhouse/configs": "^1.27.1",
        "@kant2002-diia-inhouse/crypto": "^1.7.0",
        "@kant2002-diia-inhouse/diia-logger": "^2.9.0",
        "@kant2002-diia-inhouse/errors": "^1.4.2",
        "@kant2002-diia-inhouse/eslint-config": "^3.5.0",
        "@kant2002-diia-inhouse/genproto": "^1.9.2",
        "@kant2002-diia-inhouse/test": "^5.1.1",
        "@kant2002-diia-inhouse/types": "^5.0.1",
        "@kant2002-diia-inhouse/utils": "^2.36.0",
        "@kant2002-diia-inhouse/validators": "^1.13.1",
        "@types/bson": "4.2.0",
        "@types/node": "20.10.5",
        "eslint": "8.56.0",
        "jest": "29.7.0",
        "lockfile-lint": "4.12.1",
        "madge": "6.1.0",
        "prettier": "3.1.1",
        "rimraf": "5.0.5",
        "semantic-release": "21.1.2",
        "ts-node": "10.9.2",
        "ts-patch": "3.1.1",
        "type-fest": "4.8.3",
        "typescript": "5.3.3"
    },
    "release": {
        "extends": "@kant2002-diia-inhouse/configs/dist/semantic-release/package",
        "branches": [
            "main"
        ]
    },
    "commitlint": {
        "extends": "@kant2002-diia-inhouse/configs/dist/commitlint"
    },
    "eslintConfig": {
        "extends": "@kant2002-diia-inhouse/eslint-config",
        "overrides": [
            {
                "files": [
                    "*.ts"
                ],
                "parserOptions": {
                    "project": [
                        "./tsconfig.json",
                        "./tests/tsconfig.json"
                    ]
                }
            }
        ]
    },
    "jest": {
        "preset": "@kant2002-diia-inhouse/configs/dist/jest"
    },
    "prettier": "@kant2002-diia-inhouse/eslint-config/prettier",
    "madge": {
        "tsConfig": "./tsconfig.json"
    }
}
